<h3><a class="btn btn-secondary" href="/sv"><i class="fa fa-plus"></i> Thêm mới </a> Danh sách SV </h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Mã SV</th>
            <th>Tên SV</th>
            <th>Lớp</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.masv}}</td>
            <td>{{this.tensv}}</td>
            <td>{{this.lop}}</td>
            <td>
                <a href="/sv/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/sv/delete/{{this._id}}" onclick="return confirm('Bạn muốn xóa bản ghi này?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>